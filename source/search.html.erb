                    <%= partial "components/home/_tools__button", locals: { url:"#", alt: "Todo", active: true  } %>
                </li>
                <li>
                    <%= partial "components/home/_tools__button", locals: { url:"#", alt: "Noticias", active: false } %>
                </li>
                <li>
                    <%= partial "components/home/_tools__button", locals: { url:"#", alt: "Herramientas", active: false  } %>
                </li>
                <li>
